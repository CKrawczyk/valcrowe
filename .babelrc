{
  "presets": ["es2015", "react"],
  "plugins": ["transform-object-rest-spread", "add-module-exports"],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  },
}
